{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:unit": "jest --config ./test/jest.unit.json",
    "test:integration": "jest --config ./test/jest.integration.json",
    "test:ci": "pnpm test:unit && pnpm test:integration && pnpm test:e2e",
    "redis:start": "docker-compose -f docker-compose.dev.yml up -d redis",
    "redis:stop": "docker-compose -f docker-compose.dev.yml down",
    "dev": "pnpm redis:start && pnpm start:dev",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:studio": "prisma studio",
    "postinstall": "prisma generate",
    "user:reset-password": "ts-node src/scripts/user-management.ts reset-password",
    "user:list": "ts-node src/scripts/user-management.ts list",
    "user:management": "ts-node src/scripts/user-management.ts",
    "user:create-admin": "ts-node src/scripts/user-management.ts create-admin",
    "seed": "ts-node prisma/seeds/master-seeder.ts",
    "seed:all": "pnpm run seed",
    "seed:mesas": "ts-node prisma/seed-mesas.ts",
    "seed:ordenes": "ts-node prisma/seed-ordenes.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seeds/master-seeder.ts"
  },
